firedamage <- read.table("C:/Users/Caoimhe/Documents/College/Second Year/Statistics/Labs/firedamage.txt", header = TRUE)
plot(firedamage$distance, firedamage$damage, main = "Fire Damage vs Distance", xlab = "Distance (km)", ylab = "Damage (1000 euro)")
cor <- cor(firedamage$damage, firedamage$distance)
reg <- lm(damage ~ distance, data = firedamage)
summary(reg)
sqrt(cor)
predict(reg, data.frame(distance = 4.2))
plot(firedamage$distance, firedamage$damage, main = "Fire Damage vs Distance", xlab = "Distance (km)", ylab = "Damage (1000 euro)")
lines(firedamage$distance, reg$fitted.values)

ojuice<- read.table("C:/Users/Caoimhe/Documents/College/Second Year/Statistics/Labs/ojuice.txt", header = TRUE)

plot(ojuice$Pectin, ojuice$Sweetness, main = "Sweetness vs Pectin", xlab = "Pectin", ylab = "Sweetness")
lines(ojuice$Sweetness, reg$fitted.values)
abline(lm(Sweetness ~ Pectin, data = ojuice))
cor1 <- cor(ojuice$Sweetness, ojuice$Pectin)
reg1 <- lm(Sweetness ~ Pectin, data = ojuice)
summary(reg1) 
predict1 <- predict(reg1, data.frame(Pectin = 330))
